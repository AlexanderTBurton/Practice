class EnglishBD extends Dog {
  private int iq;
  EnglishBD() {
    iq = (int)(Math.random()*5);
  }
  
  EnglishBD(int iq_) {
    iq = iq_;
  }
  
  public int getIQ() {
    return iq;
  }
  
  public void setIQ(int iq_) {
    iq = iq_;
  }
  
  public void bark() {
    System.out.println("bowwowww");
  }
}
