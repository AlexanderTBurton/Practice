class Dog {
  private int dHeight;
  private float weight;
  
  Dog() {
    dHeight = 5;
    weight = 10;
  }
  
  Dog(int dHeight_,float weight_) {
    dHeight = dHeight_;
    weight = weight_;
  }
  
  public int getHeight() {
    return dHeight;
  }
  
  public void setHeight(int dHeight_) {
    dHeight = dHeight_;
  }
  
  public float getWeight() {
    return weight;
  }
  
  public void setWeight(float weight_) {
    weight = weight_;
  }
  
  public void bark() {
    System.out.println("Woof.");
  }
}
